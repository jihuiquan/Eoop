<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:id="@+id/main"    android:layout_width="fill_parent"    android:layout_height="fill_parent"    android:focusable="true"    android:focusableInTouchMode="true"    android:orientation="vertical" >    <include layout="@layout/comm_topbar_style_1" />    <include layout="@layout/commen_search" />    <FrameLayout        android:id="@+id/expand_view"        android:layout_width="match_parent"        android:layout_height="match_parent" >        <com.movitech.im.summit.tree.IphoneTreeView            xmlns:swipe="http://schemas.android.com/apk/res-auto"            android:id="@+id/iphone_tree_view"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:cacheColorHint="@android:color/transparent"            android:divider="@color/list_item_line"            android:dividerHeight="@dimen/dp_1"            android:fadingEdge="none"            android:focusable="true"            android:groupIndicator="@null"            android:listSelector="@null"            android:scrollingCache="true"            android:transcriptMode="normal" />        <com.jianye.smart.view.SideBar            android:id="@+id/MyLetterListView01"            android:layout_width="@dimen/dp_25"            android:layout_height="wrap_content"            android:layout_gravity="right|center_vertical"            android:layout_marginBottom="@dimen/dp_10"            android:layout_marginRight="@dimen/dp_4"            android:layout_marginTop="@dimen/dp_10" />    </FrameLayout>    <LinearLayout        android:id="@+id/search_view"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="vertical"        android:visibility="gone" >        <TextView            android:layout_width="wrap_content"            android:layout_height="@dimen/dp_32"            android:gravity="center_vertical"            android:paddingLeft="@dimen/dp_10"            android:text="@string/contact_str"            android:textColor="@color/grey"            android:textSize="@dimen/sp_16" />        <ListView            android:id="@+id/search_list"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:divider="@color/list_item_line"            android:dividerHeight="@dimen/dp_1"            android:fadingEdge="none"            android:focusable="true" >        </ListView>    </LinearLayout></LinearLayout>