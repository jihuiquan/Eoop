<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/welcome4_body"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EBF8FF"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/bgn"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_154"
        android:layout_marginTop="@dimen/dp_160"
        android:scaleType="fitXY"
        android:src="@drawable/bgn_4" />

    <View
        android:id="@+id/view1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/bgn"
        android:background="#13A1AB" />

    <ImageView
        android:id="@+id/welcome4_people4"
        android:layout_width="@dimen/dp_89"
        android:layout_height="@dimen/dp_123"
        android:layout_above="@+id/view1"
        android:layout_marginLeft="@dimen/dp_100"
        android:scaleType="fitXY"
        android:src="@drawable/people_4" />

    <ImageView
        android:id="@+id/welcome4_imageView"
        android:layout_width="@dimen/dp_150"
        android:layout_height="@dimen/dp_150"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_20"
        android:src="@drawable/ico_circle" />

    <ImageView
        android:layout_width="@dimen/dp_35"
        android:layout_height="@dimen/dp_35"
        android:layout_marginLeft="@dimen/dp_184"
        android:layout_marginTop="@dimen/dp_76"
        android:src="@drawable/ico_4" />

    <ImageView
        android:id="@+id/welcome4_imageView1"
        android:layout_width="@dimen/dp_45"
        android:layout_height="@dimen/dp_45"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dp_140"
        android:layout_marginTop="@dimen/dp_114"
        android:src="@drawable/ico_4_1" />

    <ImageView
        android:id="@+id/welcome4_imageView2"
        android:layout_width="@dimen/dp_36"
        android:layout_height="@dimen/dp_36"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dp_165"
        android:layout_marginTop="@dimen/dp_50"
        android:src="@drawable/ico_4_2" />

    <ImageView
        android:id="@+id/welcome4_imageView3"
        android:layout_width="@dimen/dp_30"
        android:layout_height="@dimen/dp_30"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dp_54"
        android:layout_marginTop="@dimen/dp_115"
        android:src="@drawable/ico_4_3" />

    <ImageView
        android:layout_width="@dimen/dp_246"
        android:layout_height="@dimen/dp_82"
        android:layout_below="@id/bgn"
        android:layout_marginLeft="@dimen/dp_26"
        android:layout_marginTop="@dimen/dp_40"
        android:src="@drawable/ico_word_4" />

</RelativeLayout>